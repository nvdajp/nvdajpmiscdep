%	daR'sad
[	beRAketbAz
]	beRAketbaste:
(	paRAntezbAz
)	paRAntezbaste:
*	setARe:
//this code is recognized as (ZWNJ) U+200c (half space) at eSpeak 1.46.22 and later (ZWNJ) is recognized as hyphen
	_-	nim_fAsele:
.	noqte:
:	'donoqte:
<	:alAmate'kutSektaR
=	:alAmate'mosAvi
>	:alAmate'bozoRgtaR
